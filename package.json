    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "eslint": "^2.13.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "^5.2.2",
    "nyan-progress-webpack-plugin": "1.1.4",
    "react-hot-loader": "^3.0.0-beta.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "css-loader": "0.23.1",
